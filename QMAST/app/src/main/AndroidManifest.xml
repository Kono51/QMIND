<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.joseph.qmast">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Diagnostics"
            android:label="@string/title_activity_diagnostics"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Autonomous"
            android:label="@string/title_activity_autonomous"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".CellControl"
            android:label="@string/title_activity_cell_control"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".RC"
            android:label="@string/title_activity_rc"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Stationary"
            android:label="@string/title_activity_stationary"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>
